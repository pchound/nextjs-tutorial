#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/Files not on HDD1/Projects/Code/nextjs-tutorial/chapter1/nextjs-tutorial/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules/node-gyp-build/node_modules:/mnt/f/Files not on HDD1/Projects/Code/nextjs-tutorial/chapter1/nextjs-tutorial/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules:/mnt/f/Files not on HDD1/Projects/Code/nextjs-tutorial/chapter1/nextjs-tutorial/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/Files not on HDD1/Projects/Code/nextjs-tutorial/chapter1/nextjs-tutorial/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules/node-gyp-build/node_modules:/mnt/f/Files not on HDD1/Projects/Code/nextjs-tutorial/chapter1/nextjs-tutorial/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules:/mnt/f/Files not on HDD1/Projects/Code/nextjs-tutorial/chapter1/nextjs-tutorial/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/bin.js" "$@"
else
  exec node  "$basedir/../node-gyp-build/bin.js" "$@"
fi
